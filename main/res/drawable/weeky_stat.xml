<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#00FFFFFF" />
            <stroke
                android:width="3dp"
                android:color="#F3AA51" />
            <size
                android:width="30dp"
                android:height="30dp" />
            <corners
                android:radius="50dp" />
        </shape>
    </item>
</selector>